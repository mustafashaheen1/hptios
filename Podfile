def myPods
    pod 'GoogleSignIn'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'IQKeyboardManager'
    pod 'Google/SignIn'
    pod 'ScanditSDK'
    #pod 'Alamofire'
end

target 'Insights-Trimble-Enterprise' do
    myPods
end

target 'ScanOut' do
    myPods
end

target 'CodeExplorer' do
    myPods
end

target 'Insights-Trimble-Enterprise Inventory' do
	myPods
end

target 'Insights-AppStore' do
    myPods
end

target 'PalletShipping-AppStore' do
	myPods
end

target 'PalletShipping-Enterprise' do
    myPods
end



